{
  "name": "procjam2015-blzrs-three_things",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "browserify www/demo.js -o www/bundle.js",
    "deploy": "git push origin master && gh-pages-deploy",
    "watch": "watchify www/demo.js -o www/bundle.js --debug --verbose",
    "test": "standard && node test.js",
    "publish": "npm run test"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "tape": "4.0.1",
    "watchify": "^3.3.1",
    "gh-pages-deploy": "^0.3.0",
    "standard": "4.5.4"
  },
  "gh-pages-deploy": {
    "staticpath": "www",
    "prep": [
      "test",
      "build"
    ],
    "noprompt": false
  },
  "license": "MIT",
  "description": "procjam2015-blzrs-three_things ----------------",
  "author": "Cole Willsea <coleww@gmail.com> (http://colewillsea.com/)"
}
